pipeline {
    agent any

    stages {
        stage('checkout') {
            steps {
               git url : 'https://github.com/GJohannes/baJenkins'
            }
        }
         stage('Build') {
            steps {
            	withMaven(jdk: 'jdk8', maven: 'maven3') {
            		sh "mvn make"
        		}
            }
        }
    }
}